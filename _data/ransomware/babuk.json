    "MITRE": {
      "Name": "Babuk",
      "ID": "S0638",
      "Description": "[Babuk](https://attack.mitre.org/software/S0638) is a Ransomware-as-a-service (RaaS) malware that has been used since at least 2021. The operators of [Babuk](https://attack.mitre.org/software/S0638) employ a \"Big Game Hunting\" approach to targeting major enterprises and operate a leak site to post stolen data as part of their extortion scheme.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)(Citation: CyberScoop Babuk February 2021)",
      "link": "https://attack.mitre.org/software/S0638",
      "os": "Linux, Windows",
      "citations": [
        "Trend Micro Ransomware February 2021",
        "Sogeti CERT ESEC Babuk March 2021",
        "McAfee Babuk February 2021",
        "Medium Babuk February 2021"
      ],
      "GroupName": "",
      "GroupId": "",
      "techniques": [
        {
          "id": "T1486",
          "name": "Data Encrypted for Impact",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can use ChaCha8 and ECDH to encrypt data.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)(Citation: Medium Babuk February 2021)(Citation: Trend Micro Ransomware February 2021)"
        },
        {
          "id": "T1140",
          "name": "Deobfuscate/Decode Files or Information",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) has the ability to unpack itself into memory using XOR.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: Medium Babuk February 2021)"
        },
        {
          "id": "T1562.001",
          "name": "Disable or Modify Tools",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can stop anti-virus services on a compromised host.(Citation: Sogeti CERT ESEC Babuk March 2021)"
        },
        {
          "id": "T1083",
          "name": "File and Directory Discovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) has the ability to enumerate files on a targeted system.(Citation: McAfee Babuk February 2021)(Citation: Trend Micro Ransomware February 2021)"
        },
        {
          "id": "T1490",
          "name": "Inhibit System Recovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) has the ability to delete shadow volumes using <code>vssadmin.exe delete shadows /all /quiet</code>.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)"
        },
        {
          "id": "T1106",
          "name": "Native API",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can use multiple Windows API calls for actions on compromised hosts including discovery and execution.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)(Citation: Medium Babuk February 2021)"
        },
        {
          "id": "T1135",
          "name": "Network Share Discovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) has the ability to enumerate network shares.(Citation: Sogeti CERT ESEC Babuk March 2021)"
        },
        {
          "id": "T1057",
          "name": "Process Discovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) has the ability to check running processes on a targeted system.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)(Citation: Trend Micro Ransomware February 2021)"
        },
        {
          "id": "T1489",
          "name": "Service Stop",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can stop specific services related to backups.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)(Citation: Trend Micro Ransomware February 2021)"
        },
        {
          "id": "T1027.002",
          "name": "Software Packing",
          "description": "Versions of [Babuk](https://attack.mitre.org/software/S0638) have been packed.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)(Citation: Medium Babuk February 2021)"
        },
        {
          "id": "T1082",
          "name": "System Information Discovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can enumerate disk volumes, get disk information, and query service status.(Citation: McAfee Babuk February 2021)"
        },
        {
          "id": "T1049",
          "name": "System Network Connections Discovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can use \u201cWNetOpenEnumW\u201d and \u201cWNetEnumResourceW\u201d to enumerate files in network resources for encryption.(Citation: McAfee Babuk February 2021)"
        },
        {
          "id": "T1007",
          "name": "System Service Discovery",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) can enumerate all services running on a compromised host.(Citation: McAfee Babuk February 2021)"
        },
        {
          "id": "T1059.003",
          "name": "Windows Command Shell",
          "description": "[Babuk](https://attack.mitre.org/software/S0638) has the ability to use the command line to control execution on compromised hosts.(Citation: Sogeti CERT ESEC Babuk March 2021)(Citation: McAfee Babuk February 2021)"
        }
